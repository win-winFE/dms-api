{
  "name": "dms-api",
  "version": "0.1.0",
  "description": "提供dms数据访问",
  "private": false,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-cors": "^2.1.2",
    "egg-redis": "^2.2.0",
    "egg-scripts": "^2.5.0",
    "egg-sequelize": "^4.2.0",
    "ioredis": "^4.5.1",
    "mysql2": "^1.6.4"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "keywords": [
    "dms",
    "api"
  ],
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-dms-api --port=7102",
